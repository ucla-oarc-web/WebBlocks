<code class="text-primary"><b><%= name %></b></code>
<small>
<br><%= description %>
<% if(typeof definitions !== 'undefined'){ %>
    <br><strong class="text-success">defines</strong> <code><%= definitions %></code>
<% } %>
<% if(typeof definitions_js !== 'undefined'){ %>
    <br><strong class="text-success">defines (JS)</strong> <code><%= definitions_js %></code>
<% } %>
<% if(typeof uses !== 'undefined'){ %>
    <br><strong class="text-important">uses</strong> <code><%= uses %></code>
<% } %>
<% if(typeof supports !== 'undefined'){ %>
    <br><strong class="text-tertiary">supports</strong> <code><%= supports %></code>
<% } %>
<% if(typeof submodules !== 'undefined'){ %>
    <br><strong class="text-info">submodules</strong> <code><%= submodules %></code>
<% } %>
</small>